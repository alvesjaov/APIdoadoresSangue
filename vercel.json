{
    "version": 2,
    "builds": [
        {
            "src": "/server.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/server.js"
        },
        {
            "src": "/swagger.json",
            "dest": "/swagger.json"
        }
    ]
}
